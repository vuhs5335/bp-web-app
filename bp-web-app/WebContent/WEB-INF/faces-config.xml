<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<navigation-rule>
		<from-view-id>*</from-view-id>
		 <navigation-case>
	       <from-outcome>login</from-outcome>
	       <to-view-id>/login.xhtml</to-view-id>
	       <redirect/>
	    </navigation-case>
	    <navigation-case>
	       <from-outcome>readings</from-outcome>
	       <to-view-id>/private/readings.xhtml</to-view-id>
	       <redirect/>
	    </navigation-case>
	     <navigation-case>
	       <from-outcome>addreading</from-outcome>
	       <to-view-id>/private/addreading.xhtml</to-view-id>
	       <redirect/>
	    </navigation-case>
	    <navigation-case>
	       <from-outcome>dashboard</from-outcome>
	       <to-view-id>/private/dashboard.xhtml</to-view-id>
	       <redirect/>
	    </navigation-case>
	    <navigation-case>
	       <from-outcome>unauthorized</from-outcome>
	       <to-view-id>/unauthorized.xhtml</to-view-id>
	       <redirect/>
	    </navigation-case>
	    <navigation-case>
	       <from-outcome>tools</from-outcome>
	       <to-view-id>/private/tools.xhtml</to-view-id>
	       <redirect/>
	    </navigation-case>
	    <navigation-case>
	       <from-outcome>apitool</from-outcome>
	       <to-view-id>/private/admin/apitool.xhtml</to-view-id>
	       <redirect/>
	    </navigation-case>
	</navigation-rule>
</faces-config>
